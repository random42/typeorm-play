    "typeorm": "^0.2.31",
    "typeorm-naming-strategies": "^2.0.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/node": "^14.14.31",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
